    "type": "git",
    "url": "git+https://github.com/MarianRaphael/node-red-contrib-unide-ppmp.git"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "fs": "^0.0.2"
  },
  "keywords": [
    "node-red",
    "ppmp",
    "unide",
    "bosch"
  ],
